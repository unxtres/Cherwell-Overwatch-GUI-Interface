<Window x:Class="CherwellOVerwatch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CherwellOVerwatch"
        mc:Ignorable="d"
        Title="MainWindow" Height="484" Width="860">
    <Grid Margin="0,0,0,33">
        <Button Content="Button" HorizontalAlignment="Left" Margin="699,9,0,0" VerticalAlignment="Top" Click="Button_Get_Token"/>
        <TextBox x:Name="regkey" HorizontalAlignment="Left" Margin="400,10,0,0" TextWrapping="Wrap" Text="Registration Key" VerticalAlignment="Top" Width="282"/>
        <TextBox x:Name="result" HorizontalAlignment="Left" Margin="400,55,0,0" TextWrapping="Wrap" Text="Token" VerticalAlignment="Top" Width="344" Height="105"/>
        <Button Content="App Server Settings" HorizontalAlignment="Left" Margin="115,29,0,0" VerticalAlignment="Top" Click="Button_AppServerSettings"/>
        <TextBox x:Name="settings" HorizontalAlignment="Left" Margin="74,108,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="240" Height="300"/>
    </Grid>
</Window>
